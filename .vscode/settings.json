{
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.formatOnSave": true,
    "eslint.enable": true,
    "eslint.alwaysShowStatus": true,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "files.exclude": {
      "**/.DS_Store": true,
      "**/.git": true,
      "dist": true,
      "node_modules": true,
      "coverage-ts": true,
      "yarn-error.log": true
    },
    "search.exclude": {
      "yarn.lock": true
    },
  }
  